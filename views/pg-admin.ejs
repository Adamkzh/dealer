<body class="light-text-color">

<h1>Admin</h1>

<p>Dealer To Individual Transaction</p>
<table class="table-fill">
  <thead>
  <tr>

    <th>Dealer Name</th>
    <th>DealerUsername</th>
    <th>Dealer ID</th>
    <th>Individual FirstName</th>
    <th>Individual LastName</th>
    <th>Individual ID</th>
    <th>Car ID</th>
	<th>Manufacture</th>
    <th>Model</th>
    <th>Year</th>
	<th>Transaction ID</th>
    <th>Date Time</th>
    <th>Price</th>
	<th></th>
	
  </tr>
  </thead>
  <tbody class="table-hover">
  <% for(var i = 0; i < dealerToIndividualTransaction.length; i++) {%>
    <tr>
      <td><%=dealerToIndividualTransaction[i].DealerName%></td>
      <td><%=dealerToIndividualTransaction[i].DealerUsername%></td>
      <td><%=dealerToIndividualTransaction[i].DealerId%></td>
      <td><%=dealerToIndividualTransaction[i].FirstName%></td>
      <td><%=dealerToIndividualTransaction[i].LastName%></td>
	  <td><%=dealerToIndividualTransaction[i].IndividualID%></td>
	  
	  <td><%=dealerToIndividualTransaction[i].CarID%></td>
      <td><%=dealerToIndividualTransaction[i].Manufacture%></td>
      <td><%=dealerToIndividualTransaction[i].Model%></td>
      <td><%=dealerToIndividualTransaction[i].Year%></td>
	  
      <td><%=dealerToIndividualTransaction[i].TransactionID%></td>
      <td><%=dealerToIndividualTransaction[i].DateTime%></td>
      <td><%=dealerToIndividualTransaction[i].Price%></td>
	  <td><button onclick="handleDelete(event)">Delete</button></td>
    </tr>
  <% } %>
  </tbody>
</table>


