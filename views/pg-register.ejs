                <div id="register-individual">
                    <% include ./individual-register.ejs %>
                </div>
            </div>
            <div class="col-lg-5 container">
                <table>
                	<tr>
				    <td>&nbsp;&nbsp;<button class="social-signin facebook">Log in with facebook</button></td>
				  	</tr>
				  	<tr>
